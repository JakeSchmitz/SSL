<h3><%= @doc['ssltitle'] %></h3>
<h5>By <%= @doc['sslauthor'] %></h5>
<p>Additional Info:</p>
<table style="width:100%">
  <tr>
  	<th>Attribute</th> 
    <th>Value</th>
  </tr>
	<% @doc.each do |k, v| %>
		<% if k[0] == 'u' then k = k[2..-2] end %>
		<tr>
			<td><%= k.gsub(/\u0080-\u00ff/,'') %></td> 
			<td><%= v %></td> 
		</tr>
	<% end %>
